import React, {Component} from 'react';

class Header extends Component {
    render() {
        let title = "";
        if (this.props.header) {
            title = this.props.header.title;
        }
        return (
            <div className="header">
                <div className="title">{title}</div>
            </div>
        );
    }
}

module.exports = Header;